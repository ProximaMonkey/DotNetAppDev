<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel> <!--Установка привязки на основе TCP-->
    <services>
      <service name="MagicEightBallServiceLib.MagicEightBallService"> <!--address="http://localhost/MagicEightBallService"-->       
        <endpoint address=""
                  binding="netTcpBinding"
                  contract="MagicEightBallServiceLib.IEightBall"/>        
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8090/MagicEightBallService"/>
          </baseAddresses>
        </host>
      </service>
    </services>    
  </system.serviceModel>
</configuration>