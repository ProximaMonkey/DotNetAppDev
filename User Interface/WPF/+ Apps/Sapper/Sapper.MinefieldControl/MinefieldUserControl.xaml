            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Viewbox Grid.Row="0" Grid.Column="0" MaxHeight="700" x:Name="MinefieldViewBox"/>
    </Grid>-->
</UserControl>